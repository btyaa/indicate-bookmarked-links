{
  "author": "igorlogius",
  "background": {
	"scripts": ["background.js"]
  },
  "content_scripts": [
  {
    "all_frames": true,
    "matches": ["*://*/*"],
    "js": ["content-script.js"],
    "css": ["content-script.css"]
  }
  ],
  "description": "indicate on hover if a link has already been bookmarked or not",
  "icons": {
      "128": "icon.png"
  },
  "manifest_version": 2,
  "name": "indicate-if-link-is-bookmarked",
  "permissions": [
    "bookmarks"
  ],
  "version": "1.0.2"
}
